<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.example.mapper.AccountMapper">


    <select id="getAllAccount" resultType="com.example.pojo.Account">
      select * from userinfo
    </select>

    <select id="getById" resultType="com.example.pojo.Account">
               select * from userinfo WHERE rid=#{rid}
    </select>


</mapper>

